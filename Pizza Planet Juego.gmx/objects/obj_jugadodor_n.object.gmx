///MOVIMIENTO DEL JUGADOR 

// Entradas de Teclado
key_right = keyboard_check(vk_right);           // Revisar si se preciona la tecla de la derecha
key_left = keyboard_check(vk_left);             // Revisar si se preciona la tecla de la izquierda
//key_left = 0;
key_space = keyboard_check_pressed(vk_up);   // Revisar si se preciona la tecla de la espacio

//Operaciones con la entrada de teclado 
move = key_right - key_left;                    // 
hsp = move * movespeed;

if (x &gt; sprite_width/2 &amp;&amp; hsp &lt; 0){
    x += hsp;
}else if (x &lt; room_width-sprite_width/2 &amp;&amp; hsp &gt; 0){
    x += hsp;
}

if (vsp &lt; 10) {vsp += grav;}

if(place_meeting(x, y+1, obj_bloques)){
    vsp = key_space * -jumpspeed;
}

// Colision Vertical
if(place_meeting(x, y+vsp, obj_bloques) || place_meeting(x, y+vsp, obj_plataforma) ){
    while(!place_meeting(x, y + sign(vsp),obj_bloques)){
        y += sign(vsp);
    }
    vsp = 0;
}

y += vsp;


/*if (y == 416) {
